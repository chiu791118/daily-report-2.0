# Gemini API (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# YouTube Data API (Required)
YOUTUBE_API_KEY=your_youtube_api_key_here

# NewsAPI (Optional - backup news source)
NEWS_API_KEY=your_newsapi_key_here

# Alpha Vantage (Optional - economic indicators)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Notion (Phase 2 - Optional)
NOTION_API_KEY=your_notion_api_key_here
NOTION_DATABASE_ID=your_database_id_here

# Webshare Proxy (Optional - for YouTube transcript fetching)
# Get free account at https://www.webshare.io/
WEBSHARE_PROXY_USERNAME=your_webshare_username
WEBSHARE_PROXY_PASSWORD=your_webshare_password


# Financial Modeling Prep (FMP) - earnings & universe
FMP_API_KEY=your_fmp_api_key_here

# Trading Economics (economic calendar)
TRADING_ECONOMICS_API_KEY=your_trading_economics_key_here

# Optional calendars/universe settings
ECONOMIC_CALENDAR_COUNTRIES=United States
ECONOMIC_CALENDAR_IMPORTANCE_MIN=2
UNIVERSE_INCLUDE_SP500=true
UNIVERSE_ETF_HOLDINGS=QQQ,IWM,SOXX
UNIVERSE_CACHE_HOURS=24
